    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-links": "^5.2.6",
    "@storybook/addons": "^5.2.6",
    "@storybook/vue": "^5.2.6",
    "babel-eslint": "^10.0.3",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-vue-jsx": "^4.0.1",
    "babel-preset-vue": "^2.0.2",
    "chai": "^4.2.0",
    "chai-samsam": "0.0.2",
    "chalk": "^3.0.0",
    "copy-webpack-plugin": "^5.0.5",
    "cpy-cli": "^2.0.0",
