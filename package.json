  "lint-staged": {
    "*.js": [
      "prettier --write 'src/**/*.js'",
      "eslint --ext .js --fix src",
      "git add"
    ],
    "*.vue": [
      "prettier --write 'src/**/*.vue'",
      "eslint --ext .vue --fix src",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": "10.x",
    "npm": ">= 6.0.0"
  }
}
